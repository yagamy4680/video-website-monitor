{
  "manifest_version": 2,

  "name": "Video Website Monitor",
  "description": "This extension monitors video website visits, such as Youtube",
  "version": "1.0",

  "background": {
   "scripts": ["background.js"]
 },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://10.42.0.1:3000",
    "http://127.0.0.1:3000",
    "*://*.youtube.com/"
  ]
}
